rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    
    // Seuls les utilisateurs authentifiés peuvent lire et écrire dans le dossier "uploads".
    match /uploads/{userId}/{allPaths=**} {
      allow read;
      allow write: if request.auth != null && request.auth.uid == userId;
    }
    
  }
}
